{
    "name": "Google Calendar Guests Can Modify Event By Default",
    "version": "0.1",
    "icons": {
        "16": "/img/icons/icon_16.png",
        "48": "/img/icons/icon_48.png",
        "128": "/img/icons/icon_128.png"
    },

    "description": "Enables modify event setting for google calendar by default.",

    "content_scripts": [
        {
            "matches": ["https://www.google.com/calendar/*"],
            "js": [
                    "/js/content_script.js"
                ]
        }
    ],

    "manifest_version": 2
}
